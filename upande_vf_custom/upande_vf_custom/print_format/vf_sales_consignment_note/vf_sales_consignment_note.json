 "doctype": "Print Format",
 "font_size": 14,
 "html": "<link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css\" rel=\"stylesheet\" integrity=\"sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH\" crossorigin=\"anonymous\">\n\n<div class=\"container\">\n  <div class=\"row justify-content-between\">\n    <div class=\"col-auto mr-auto\">\n    <div>\n      {%- if doc.meta.is_submittable and doc.docstatus==0-%}\n      <div class=\"alert alert-info text-center\">\n          <h6 style=\"margin: 0px;\">{{ _(\"PROFORMA\") }}</h6>\n      </div>\n      {%- endif -%}\n      \n      {%- if doc.meta.is_submittable and doc.docstatus==2-%}\n      <div class=\"alert alert-danger text-center\">\n          <h6 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h6>\n      </div>\n      {%- endif -%}\n      \n      {%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n          {% if letter_head and not no_letterhead %}\n          <div class=\"letter-head\">{{ letter_head }}</div>\n          {% endif %}\n          \n          {% if max_pages > 1 %}\n          <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n          {% endif %}\n      {%- endmacro -%}\n  \n      {{ add_header(0,1,doc,letter_head, no_letterhead) }}\n    </div>\n\n    <br>\n    {% if doc.customer_address %}\n    \n    {% set address = frappe.get_doc(\"Address\", doc.customer_address) %}\n    \n    {% endif %}\n    <div>\n        <p><strong>Customer Information</strong></p>\n          <p>{{ doc.customer_name }}<br>{% if doc.tax_id %}PIN: {{ doc.tax_id }} {% endif %}</p>\n          {% if address %}\n          <p>{%if address.pincode%}{{ address.pincode }}{%endif%}, {%if address.address_line1%}{{ address.address_line1 }}{%endif%}</p>\n          <p>{%if address.address_line2%}{{ address.address_line2 }}{%endif%}</p>\n          <p>{%if address.phone%}{{ address.phone }}{%endif%}</p>\n          {% endif %}\n         \n    </div>\n  </div>\n  <div class=\"col-auto\">\n      <h6>CONSIGNMENT NOTE</h6>\n      <p>Delivery Date: {{ doc.get_formatted(\"delivery_date\") or '' }}<br><br><strong>Consignemt Note Number</strong><br>{{ doc.name }}</p>\n      <br>\n      <p><strong>Vehicle</strong><br>{{ doc.truck_warehouse or '' }}<br><br><strong>Driver</strong><br>{{ doc.driver_name }}</p>\n      <br>\n  </div>\n  </div>\n</div>\n\n    <br>\n    <hr>\n    \n<div class=\"container\">\n    <p class=\"\"><strong>Delivery Summary</strong></p>\n    <table class=\"table table-bordered\">\n    \t<tbody>\n    \t\t<tr>\n    \t\t\t<th style=\"width: 45%; text-align: left;\">Description</th>\n    \t\t\t<th style=\"width: 10%; text-align: center;\">Qty</th>\n    \t\t\t<th style=\"width: 15%; text-align: center;\">Unit</th>\n    \t\t\t<th style=\"width: 15%; text-align: center;\">Amount</th>\n                <th style=\"width: 15%; text-align: center;\">Rcvd Qty</th>\n    \t\t</tr>\n    \t\t{%- for row in doc.items -%}\n    \t\t<tr>\n    \t\t\t<td style=\"text-align: left;\">{{ row.item_code }}</td>\n    \t\t\t<td style=\"text-align: center;\">{{ row.qty }}</td>\n    \t\t\t<td style=\"text-align: center;\">{{ row.uom }}</td>\n    \t\t\t<td style=\"text-align: right;\">{{ frappe.utils.fmt_money(row.amount) }}</td>\n    \t\t\t<td style=\"text-align: right;\"></td>\n    \t\t</tr>\n    \t\t{%- endfor -%}\n    \t</tbody>\n    </table>\n    \n    <br>\n    <hr>\n    \n    <p class=\"\"><strong>Delivery Breakdown</strong></p>\n    <table class=\"table table-bordered\">\n    \t<tbody>\n    \t\t<tr>\n    \t\t\t<th style=\"width: 40%; text-align: left;\">Description</th>\n    \t\t\t<th style=\"width: 15%; text-align: center;\">Unit</th>\n    \t\t\t<th style=\"width: 15%; text-align: center;\">Qty</th>\n    \t\t\t<th style=\"width: 15%; text-align: center;\">Crate No</th>\n                <th style=\"width: 15%; text-align: center;\">Rcvd Qty</th>\n    \t\t</tr>\n    \t\t{%- for row in doc.crates -%}\n    \t\t<tr>\n    \t\t\t<td style=\"text-align: left;\">{{ row.item_code }}</td>\n    \t\t\t<td style=\"text-align: center;\">{{ row.uom }}</td>\n    \t\t\t<td style=\"text-align: center;\">{{ row.qty }}</td>\n    \t\t\t<td style=\"text-align: center;\">{{ row.crate_number }}</td>\n    \t\t\t<td style=\"text-align: center;\"></td>\n    \t\t</tr>\n    \t\t{%- endfor -%}\n    \t</tbody>\n    </table>\n    \n    <br>\n    <hr>\n    <p class=\"\"><strong>Victory Farms Gate Pass</strong></p>\n    <table class=\"table table-bordered\">\n    \t<tbody>\n    \t\t<tr>\n    \t\t\t<th style=\"width: 30%; text-align: left;\">Unit</th>\n    \t\t\t<th style=\"width: 25%; text-align: center;\">Time</th>\n    \t\t\t<th style=\"width: 25%; text-align: center;\">Name</th>\n    \t\t\t<th style=\"width: 20%; text-align: center;\">Signature</th>\n    \t\t</tr>\n    \t\t{%- for row in [\"Security\", \"Fleet Manager\", \"Driver\"]-%}\n    \t\t<tr>\n    \t\t\t<td style=\"text-align: left;\">{{ row }}</td>\n    \t\t\t<td style=\"text-align: right;\"></td>\n    \t\t\t<td style=\"text-align: right;\"></td>\n    \t\t\t<td style=\"text-align: right;\"></td>\n    \t\t</tr>\n    \t\t{%- endfor -%}\n    \t</tbody>\n    </table>\n    \n    <br>\n    <hr>\n    <p class=\"\"><strong>Receipt Confirmation</strong></p>\n    <table class=\"table table-bordered\">\n    \t<tbody>\n    \t\t<tr>\n    \t\t\t<th style=\"width: 30%; text-align: left;\">Party</th>\n    \t\t\t<th style=\"width: 25%; text-align: center;\">Name</th>\n    \t\t\t<th style=\"width: 25%; text-align: center;\">Time</th>\n    \t\t\t<th style=\"width: 20%; text-align: center;\">Signature</th>\n    \t\t</tr>\n    \t\t{%- for row in [\"Customer\", \"Driver\"]-%}\n    \t\t<tr>\n    \t\t\t<td style=\"text-align: left;\">{{ row }}</td>\n    \t\t\t<td style=\"text-align: right;\"></td>\n    \t\t\t<td style=\"text-align: right;\"></td>\n    \t\t\t<td style=\"text-align: right;\"></td>\n    \t\t</tr>\n    \t\t{%- endfor -%}\n    \t</tbody>\n    </table>\n    \n    <div class=\"row\">\n        <div style=\"height: 150px;\"></div>\n        <p>Official Stamp</p>\n        <hr class=\"w-50\">\n        \n    </div>\n</div>\n\n",
